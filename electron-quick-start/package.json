{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --out ./OutApp --electron-version 8.0.0 --overwrite --icon=./app/assets/img/icon.png",
    "package-win64": "electron-packager . --out ./OutApp --platform=win32 --arch=x64 --electron-version 8.0.0 --overwrite --icon=./app/assets/img/icon.png"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "element-ui": "^2.13.0"
  }
}